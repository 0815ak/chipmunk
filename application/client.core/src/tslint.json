{
    "extends": "../tslint.json",
    "rules": {
        "no-unused-expression": false,
        "no-redundant-jsdoc": false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-inferrable-types": false,
        "max-line-length": false
    }
}
